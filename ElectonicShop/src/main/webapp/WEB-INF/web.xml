<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Product</servlet-name>
  	<display-name>Product</display-name>
  	<description></description>
  	<servlet-class>com.myshop.DTO.Product</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductDto</servlet-name>
  	<display-name>ProductDto</display-name>
  	<description></description>
  	<servlet-class>com.bridgeit.electronicshop.ProductDto</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductServlet</servlet-name>
  	<display-name>ProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.bridgeit.electronicshop.Controller.ProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>showDetails</servlet-name>
  	<display-name>showDetails</display-name>
  	<description></description>
  	<servlet-class>com.bridgeit.electronicshop.Controller.showDetails</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowProductDetails</servlet-name>
  	<display-name>ShowProductDetails</display-name>
  	<description></description>
  	<servlet-class>com.bridgeit.electronicshop.Controller.ShowProductDetails</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Product</servlet-name>
  	<url-pattern>/Product</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductDto</servlet-name>
  	<url-pattern>/ProductDto</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductServlet</servlet-name>
  	<url-pattern>/ProductServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>showDetails</servlet-name>
  	<url-pattern>/showDetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowProductDetails</servlet-name>
  	<url-pattern>/ShowProductDetails</url-pattern>
  </servlet-mapping>
</web-app>
